#include<stdio.h>
int main()
{
    int n,m;
    scanf("%d",&n);
    int g[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&g[i]);
    }
    scanf("%d",&m);
    int s[m];
    for(int j=0;j<m;j++)
    {
      scanf("%d",&s[j]);
        
    }
    int i=0,j=0,c=0;
    while(i<n&&j<m)
    {
        if(s[j]>=g[i])
        {
            c++;
            j++;
            i++;
        }
        else
        {
            j++;
        }
    }
    printf("%d",c);

}
